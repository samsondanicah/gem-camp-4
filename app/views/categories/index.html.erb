<h1>Categories</h1>
<p>Find me in app/views/categories/index.html.erb</p>
<h1>Category</h1>

<ul>
  <% @categories.each do |category| %>
    <li>
      <%= category.name %>
      <%= link_to :edit, edit_category_path(category) %>
      <%= button_to :delete, category_path(category), method: :delete %>
    </li>
  <% end %>
</ul>

<%= link_to 'new', new_category_path %>
